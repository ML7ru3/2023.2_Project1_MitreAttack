<AnchorPane prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: linear-gradient(to bottom, #ff7e5f, #feb47b);" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" layoutX="51.0" layoutY="91.0" prefHeight="299.0" prefWidth="700.0" style="-fx-text-fill: black;" text="ATOMIC RED TEAM COVERAGE &amp; ANALYSIS" textAlignment="CENTER" textFill="#191f32" wrapText="true">
         <font>
            <Font name="Georgia" size="67.0" />
